<%= form_for @job do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :location %>
<%= f.text_field :location %>
<%= f.collection_select(:company_id, @companies, :id, :name) %>
<%= f.collection_select(:category_id, @categories, :id, :name) %>
<%= f.label :description %>
<%= f.text_area :description %>
<%= f.submit 'Atualizar Vaga' %>
<%= f.label :featured %>
<%= f.check_box :featured %>
<% end %>
